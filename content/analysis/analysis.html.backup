            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
                if($(e.target).text() === '판매할 상품'){
                    if(!$('#form1').hasClass('filled')){
                        $('#form1').focus();
                    }
                }else if($(e.target).text() === '기존 상품'){
